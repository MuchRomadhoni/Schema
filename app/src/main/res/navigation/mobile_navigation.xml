<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myschedule.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_jadwal"
        android:name="com.example.myschedule.ui.jadwal.JadwalFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_jadwal" />

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.example.myschedule.ui.chat.ChatFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_chat" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.myschedule.profilemahasiswaFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_profilemahasiswa" />
</navigation>
